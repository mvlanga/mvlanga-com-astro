{
	"extends": "astro/tsconfigs/strictest",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"paths": {
			"@/*": ["./src/*"]
		},
		"strictNullChecks": true,
		"allowJs": true,
		"exactOptionalPropertyTypes": false
	}
}
